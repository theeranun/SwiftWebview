# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'TTP APP' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
	
  # Pods for TTP APP
	pod 'FBSDKCoreKit'
	pod 'FBSDKShareKit'
	pod 'FBSDKLoginKit'
   

  target 'TTP APPTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'TTP APPUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
